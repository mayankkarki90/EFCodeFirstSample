{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=GamesLibrary;TrustServerCertificate=True;Integrated Security=True;"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Log-.txt",
          "rollingInterval" :  "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
